{
  "name": "htmf",
  "version": "0.5.0",
  "description": "MCTS implementation of a neat game",
  "main": "src/main.tsx",
  "type": "module",
  "scripts": {
    "build:wasm-st": "wasm-pack build --target web --profiling",
    "build:wasm-mt": "wasm-pack build --target web --profiling -- --features parallel",
    "dev": "vite",
    "test": "vitest",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "npm run build && vite preview",
    "predeploy:pages": "npm run build:wasm-st && npm run build",
    "deploy:pages": "gh-pages -d ./dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jthemphill/htmf.git"
  },
  "keywords": [
    "mcts",
    "wasm",
    "rust"
  ],
  "author": "Jeff Hemphill <jthemphill@gmail.com>",
  "license": "(MIT OR Apache-2.0)",
  "bugs": {
    "url": "https://github.com/jthemphill/htmf/issues"
  },
  "homepage": "https://jthemphill.github.io/htmf",
  "dependencies": {
    "htmf-wasm": "workspace:*",
    "react": "19.0.0-rc-45804af1-20241021",
    "react-dom": "19.0.0-rc-45804af1-20241021"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.1",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.13.0",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "@typescript-eslint/eslint-plugin": "8.11.0",
    "@vitejs/plugin-react": "^4.3.3",
    "babel-plugin-react-compiler": "19.0.0-beta-8a03594-20241020",
    "eslint": "9.13.0",
    "eslint-plugin-react-compiler": "19.0.0-beta-8a03594-20241020",
    "eslint-plugin-react-hooks": "5.0.0",
    "eslint-plugin-react-refresh": "0.4.13",
    "gh-pages": "6.2.0",
    "globals": "^15.11.0",
    "jsdom": "25.0.1",
    "jsdom-worker": "0.3.0",
    "prettier": "3.3.3",
    "typescript": "5.6.3",
    "typescript-eslint": "8.11.0",
    "vite": "5.4.10",
    "wasm-pack": "0.13.0"
  },
  "overrides": {
    "eslint-plugin-react-compiler": {
      "devDependencies": {
        "eslint": "9.13.0"
      }
    }
  }
}
